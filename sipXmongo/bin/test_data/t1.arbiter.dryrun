COMMAND:RESET_LOCAL
OS:@SIPX_SERVICEDIR@/mongod-arbiter stop
OS:/bin/rm -f /var/lib/mongodb-arbiter/local.*
OS:@SIPX_SERVICEDIR@/mongod-arbiter start
COMMAND:OBLITERATE
OS:@SIPX_SERVICEDIR@/mongod-arbiter stop
OS:rm -f /var/lib/mongodb-arbiter/mongod.lock
OS:/bin/rm -f /var/lib/mongodb-arbiter/{,_tmp,journal}/*.*
OS:/bin/chown mongodb:mongodb /var/lib/mongodb-arbiter
OS:@SIPX_SERVICEDIR@/mongod-arbiter start
COMMAND:REPAIR
OS:rm -f /var/lib/mongodb-arbiter/mongod.lock
OS:/bin/su -- mongodb /usr/bin/mongod --repair
